{
  "name": "genspark-ai-search-backend",
  "version": "1.0.0",
  "description": "AI agent backend for GenSpark AI Search MVP",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:build": "docker build -t genspark-backend .",
    "setup-mcps": "./scripts/setup-mcps.sh",
    "test-mcps": "./scripts/test-mcp-simple.sh",
    "mcp-demo": "node scripts/mcp-demo.js",
    "mcp-status": "node -e \"import('./dist/mcps/MCPManager.js').then(m => new m.MCPManager().getMCPStatus()).then(console.log)\"",
    "api-server": "tsx src/api-server.ts",
    "api-server:build": "npm run build && node dist/api-server.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.779.0",
    "@aws-sdk/s3-request-presigner": "^3.779.0",
    "@azure/storage-blob": "^12.27.0",
    "@xenova/transformers": "^2.17.2",
    "chokidar": "^3.5.3",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "docx-parser": "^0.2.1",
    "express": "^5.1.0",
    "fastify": "^4.24.3",
    "hnswlib-node": "^3.0.0",
    "ioredis": "^5.6.0",
    "mammoth": "^1.10.0",
    "ml-matrix": "^6.12.1",
    "node-cron": "^3.0.3",
    "node-fetch": "^3.3.2",
    "officegen": "^0.6.5",
    "pdf-parse": "^1.1.1",
    "pdfkit-table": "^0.1.99",
    "pg": "^8.12.0",
    "redis": "^4.6.10",
    "sharp": "^0.33.0",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "xlsx": "^0.18.5",
    "xml2js": "^0.6.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^20.19.16",
    "@types/node-cron": "^3.0.11",
    "@types/pdf-parse": "^1.1.4",
    "@types/uuid": "^9.0.7",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "eslint": "^8.53.0",
    "tsc-alias": "^1.8.8",
    "tsx": "^4.1.0",
    "typescript": "^5.2.2",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "ai",
    "search",
    "agents",
    "offline",
    "semantic-search"
  ],
  "author": "GenSpark AI Team",
  "license": "PROPRIETARY",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
